    "import cv2\n",
    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "c=0\n",
    "lab=np.zeros((265,265,3),np.uint8)\n",
    "while 1:\n",
    "    if c!=100:\n",
    "        lab[:]=np.random.rand(1,3)*256\n",
    "        cv2.imshow('lab',lab)\n",
    "        key=cv2.waitKey(50)\n",
    "        c=c+1\n",
    "\n",
    "    else:\n",
    "        break\n"
